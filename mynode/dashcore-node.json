{
  "network": "testnet",
  "port": 3001,
  "services": ["@dashevo/insight-api", "@dashevo/insight-ui", "dashd", "web"],
  "servicesConfig": {
    "@dashevo/insight-ui": {
      "routePrefix": "",
      "apiPrefix": "api"
    },
    "@dashevo/insight-api": {
      "routePrefix": "api",
      "disableRateLimiter": true
    },
    "dashd": {
      "spawn": {
        "datadir": "./data",
        "exec": "/usr/local/bin/dashd"
      }
    }
  }
}
